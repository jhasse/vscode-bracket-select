{
    "name": "bracket-select",
    "displayName": "Bracket Select",
    "description": "Quick select code between brackets, support for (),{} and [], <>",
    "version": "2.0.2",
    "publisher": "chunsen",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.34.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/wangchunsen/vscode-bracket-select.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:bracket-select.select",
        "onCommand:bracket-select.select-include"
    ],
    "extensionKind":[
      "ui"
    ],
    "main": "./out/src/bracketSelectMain",
    "contributes": {
        "commands": [
            {
                "command": "bracket-select.select",
                "title": "BraSel:Select"
            },
            {
                "command": "bracket-select.select-include",
                "title": "BraSel:SelectInclude"
            }
        ],
        "keybindings": [
            {
                "command": "bracket-select.select",
                "key": "alt+a",
                "mac": "alt+a",
                "when": "editorTextFocus"
            },
            {
                "command": "bracket-select.select-include",
                "key": "ctrl+alt+a",
                "mac": "cmd+alt+a",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "tslint -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^12.12.0",
        "@types/vscode": "^1.34.0",
        "tslint": "^5.19.0",
        "typescript": "^3.8.3"
    },
    "dependencies": {}
}
